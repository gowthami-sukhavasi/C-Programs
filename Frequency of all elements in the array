//input - 8 , 1 1 2 5 1 2 3 6
//output - 1 3
//         2 2
//         3 1
//         5 1
//         6 1

#include <iostream>
using namespace std;
 
int main() {
	int n,k,a[100], count[100];
	count[100] = {0};
	cin>>n;
	for(int i=0;i<n;i++){
		cin>>a[i];
	}
 
	for(int i=0;i<n;i++){
		count[a[i]] = count[a[i]] + 1;
	}
 
	for(int i=0;i<n;i++){
		if(count[i]>0){
			cout<<i<<" "<<count[i]<<"\n";
		}
	}
	return 0;
}
